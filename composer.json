{
	"name" : "amylian/container-basic",
        "keywords": ["container", "psr", "psr-11"],
	"description" : "Basic PSR-11 Container Implementation",
	"type" : "library",
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"authors" : [{
			"name" : "Andreas Prucha (Abexto - Helicon Software Development - Andreas Prucha / Amylian Project)",
			"email" : "andreas.prucha@gmail.com",
			"homepage" : "http://www.abexto.com",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : "^7.1",
                "amylian/amylian-utils": "*",
                "psr/container": "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^7.0"
	},
	"autoload" : {
		"psr-4" : {
			"Amylian\\Container\\Basic\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Amylian\\Container\\Basic\\Testing\\" : "tests/"
		}
	},
	"extra" : {
	}
}
